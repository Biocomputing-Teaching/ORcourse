@article{perry_precise_1982,
	title = {The {Precise} {Management} {Meaning} of a {Shadow} {Price}},
	volume = {12},
	issn = {0092-2102, 1526-551X},
	url = {https://pubsonline.informs.org/doi/10.1287/inte.12.2.61},
	doi = {10.1287/inte.12.2.61},
	abstract = {A shadow price of a resource constraint in linear programming is usually defined as the maximum price which should be paid to obtain an additional unit of re source. This definition, however, is imprecise and could lead to incorrect decisions. Be cause a shadow price already incorporates variable costs involved in the addition of the extra resource unit, shadow price should be defined as the maximum permissible amount of those costs involved in the addition which have not been included in the per-unit contribu tions of the basic decision variables. This note provides the precise meaning of a shadow price, together with examples of its use in a decision context.},
	language = {en},
	number = {2},
	urldate = {2024-04-25},
	journal = {Interfaces},
	author = {Perry, C. and Crellin, K. C.},
	month = apr,
	year = {1982},
	pages = {61--63},
	file = {perry_crellin_1982_the_precise_management_meaning_of_a_shadow_price.pdf:/home/jordivilla/Insync/jordi.villa@uvic.cat/OneDrive Biz/biblio/Interfaces/1982/perry_crellin_1982_the_precise_management_meaning_of_a_shadow_price2.pdf:application/pdf;perry_et_al_1982_the.pdf:/home/jordivilla/Insync/jordi.villa@uvic.cat/OneDrive Biz/biblio/1982/perry_et_al_1982_the2.pdf:application/pdf},
